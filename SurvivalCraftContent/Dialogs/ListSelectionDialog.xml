    <LabelWidget Name="ListSelectionDialog.Title" Color="255, 255, 255" Font="{Fonts/Pericles18}" HorizontalAlignment="Center" DropShadow="true" Margin="0, 8"/>
    <CanvasWidget Name="ListSelectionDialog.Content" Size="520, 400" ClampToBounds="true">
      <RectangleWidget FillColor="0, 0, 0, 255" OutlineColor="128, 128, 128, 128" OutlineThickness="2" />
      <ListPanelWidget Name="ListSelectionDialog.List" Direction="Vertical" HorizontalAlignment="Center" Margin="3, 3" SelectionColor="10, 70, 0, 90"/>
    </CanvasWidget>
  </StackPanelWidget>
</CanvasWidget>
